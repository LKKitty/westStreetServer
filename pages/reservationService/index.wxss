/* pages/reservationService/index.wxss */
.container .header {
  height: 280rpx;
}
.content {
  position: absolute;
  top: 0;
  width: 100%;
  height: 100vh;
  padding: 0 23rpx;
  box-sizing: border-box;
  display: flex;
  justify-content: flex-start;
  align-items: flex-start;
  flex-direction: column;
  overflow: hidden;
}
.tab {
  width: 703rpx;
  padding: 44rpx 36rpx;
  box-sizing: border-box;
  background:rgba(255,255,255,1);
  box-shadow:0rpx 18rpx 24rpx 0rpx rgba(241,241,241,0.44);
  border-radius:10rpx;
}
.date-box{
  border-radius:10rpx 10rpx 0 0;
  height: 274rpx;
  margin-top: 30rpx;
  border-bottom: 1rpx solid #F3F6F7
}
.item-content {
  position: relative;
  width: 100%;
  margin-top: 37rpx;
  flex: 1;
  overflow: auto;
}